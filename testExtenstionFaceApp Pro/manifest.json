{
   "name": "FaceApp Pro ",
   "version": "0.0.1",
   "manifest_version": 2,
   "browser_action": {
       "default_popup": "popup.html"  },
       "default_icon": {
        "128": "logo_128.png",
        "16": "logo_128.png",
        "32": "logo_128.png",
        "48": "logo_128.png"
   
},
       
   "content_scripts": [
       {
           "matches": [
               "<all_urls>"
           ],
           "js": [
               "content.js"
           ]
       }
   ],
   "icons":{
    "128": "logo_128.png",
    "16": "logo_128.png",
    "32": "logo_128.png",
    "48": "logo_128.png"
   },
   "permissions": [
       "activeTab"
      
     ]
}